@using PPB.DBManager.Models
@model List<StationModel>
<table id="StationConfigDataTbl" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th></th>
            <th>Area</th>
            <th>Section</th>
            <th>Station</th>
            <th>Stition Type</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr data-id="@m.Id" data-area="@m.AreaName" data-section="@m.SectionName" data-station="@m.StationName" data-instance="@m.StationInstance">
                <td><input type="checkbox" class="col-sm-0"/></td>
                <td>@m.AreaName</td>
                <td>@m.SectionName</td>
                <td>@m.StationName</td>
                <td>@m.StationType</td>
                <td>
                    <a href="javascript:void(0)" data-toggle="tooltip" data-placement="bottom" title="Edit" class="btn btn-xs btn-default editAction"><i class="fa fa-edit"></i> </a>
                </td>
            </tr>
        }
    </tbody>
</table>



